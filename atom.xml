eption:</span><br><span class="line">        print (__doc__)</span><br><span class="line">        os.sys.exit(1)</span><br><span class="line">        </span><br><span class="line">    (result,text) = quadratic(number, power, symbol)</span><br><span class="line">    </span><br><span class="line">    i = 0</span><br><span class="line">    total = int()</span><br><span class="line">    </span><br><span class="line">    for i in range(len(text)):</span><br><span class="line">        total += result[i]</span><br><span class="line">        print (&apos;%s = %s&apos; % (text[i], result[i]))</span><br><span class="line">        i += 1</span><br><span class="line">        if i &gt; len(text):</span><br><span class="line">            break</span><br><span class="line">    print (&apos;total: %s \nlength: %s \naverage: %s&apos; % (total, len(text), round(total/len(text))))</span><br></pre></td></tr></table></figure>]]></content>
    
